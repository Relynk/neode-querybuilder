{
  "name": "@relynk/querybuilder",
  "version": "0.1.6",
  "description": "Fluent Cypher Query Builder for NodeJS - As used in Neode",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev-packages": "nodemon",
    "lint": "eslint src --fix",
    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adam-cowley/neode-querybuilder.git"
  },
  "keywords": [
    "neo4j",
    "graph",
    "cypher",
    "gql"
  ],
  "author": "Adam Cowley <github@adamcowley.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adam-cowley/neode-querybuilder/issues"
  },
  "homepage": "https://github.com/adam-cowley/neode-querybuilder#readme",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.19",
    "sucrase": "^3.32.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "neo4j-driver": "^5.6.0",
    "save": "^2.9.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "sucrase-node src/index.ts",
    "ext": "ts"
  }
}
